parameter[geometry] = Open            

parameter[p0c]                      = 1E12
parameter[particle]                 = Positron
parameter[absolute_time_tracking]    = F

beginning[beta_a]    = 10
beginning[beta_b]    = 10


!-------------------------------------------------------

Q1: Quadrupole, cartesian_map = call::q1.cartesian_map, K1 = 0.4, field_calc = FieldMap
Q2: Quadrupole, cartesian_map = call::q1.cartesian_map, K1 = 0.4, field_calc = FieldMap
Q3: Quadrupole, cartesian_map = call::q1.cartesian_map, K1 = 0.4, field_calc = FieldMap
Q4: Quadrupole, cartesian_map = call::q4.cartesian_map, K1 = 0.4, field_calc = FieldMap
M1: Lcavity, cylindrical_map = call::m1.cylindrical_map, L = 1, RF_FREQUENCY = 1E6, GRADIENT = 2,
   GRADIENT_ERR = 1, field_calc = FieldMap
M2: Lcavity, cylindrical_map = call::m1.cylindrical_map, L = 1, RF_FREQUENCY = 1E6, GRADIENT = 2,
   field_calc = FieldMap
G1: RFcavity, grid_field = call::g1.grid_field.h5, L = 0.3, RF_FREQUENCY = 5E8, VOLTAGE = 2,
   AUTOSCALE_PHASE = F, field_calc = FieldMap
G2: RFcavity, grid_field = call::g1.grid_field.h5, L = 0.3, RF_FREQUENCY = 5E8, VOLTAGE = 2,
   AUTOSCALE_PHASE = F, field_calc = FieldMap
T1: Sbend, taylor_field = call::t1.taylor_field, K1 = 2, field_calc = FieldMap
T2: Sbend, taylor_field = call::t1.taylor_field, K1 = 2, field_calc = FieldMap
TT: Taylor, {1: 1.1 | 122}, {2: 2.2, 1 2 3 4 5 6}, {S1: 1, 0 0 0 0 0 0}

!-------------------------------------------------------
! Overlays, groups, rampers, and superimpose


!-------------------------------------------------------
! Lattice lines


LL: line = ( Q1, Q2, Q3, Q4, M1, M2, G1, G2, T1, T2, TT)

use, LL
