parameter[geometry] = Open            

parameter[p0c]                      = 9.9999986944003E8
parameter[particle]                 = Positron
parameter[absolute_time_tracking]    = F

beginning[beta_a]    = 10
beginning[beta_b]    = 10


!-------------------------------------------------------

XX: Quadrupole, field_overlaps = {YY}, L = 1
TT1: Quadrupole, L = 1
A: Quadrupole, field_overlaps = {B, C, YY}, L = 1
B: Quadrupole, L = 1
MA: Marker
MB: Marker
C: Quadrupole, L = 1
GG: girder = {A,B,C}

!-------------------------------------------------------
! Branch: YLIN

YY: Quadrupole, field_overlaps = {XX}, L = 1

!-------------------------------------------------------
! Branch: Z2

slave_drift_2_1: drift, l = 0.5
slave_drift_2_3: drift, l = 0.5
slave_drift_2_4: drift, l = 0.5
slave_drift_2_6: drift, l = 0.5
D: Drift, L = 10

!-------------------------------------------------------
! Overlays, groups, rampers, and superimpose

superimpose, element = A, ref = slave_drift_2_1, offset = 0.25
superimpose, element = B, ref = slave_drift_2_4, offset = 0.25

!-------------------------------------------------------
! Lattice lines


!-------------------------------------------------------

multi_line_01: line[multipass] = ( slave_drift_2_1, MA, slave_drift_2_3, slave_drift_2_4, MB,
   slave_drift_2_6, C)

XLIN: line = ( XX, TT1)

YLIN: line = ( YY)

YLIN[geometry] = Open
YLIN[beta_a]   = 10
YLIN[beta_b]   = 10
YLIN[particle] = Positron
YLIN[p0c]      = 9.9999986944003E8

Z2: line = ( multi_line_01, D, multi_line_01)

Z2[geometry] = Open
Z2[beta_a]   = 10
Z2[beta_b]   = 10
Z2[particle] = Positron
Z2[p0c]      = 9.9999986944003E8

use, XLIN, YLIN, Z2
