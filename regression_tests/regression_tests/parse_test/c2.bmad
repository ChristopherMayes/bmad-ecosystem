parameter[geometry] = Open            

parameter[p0c]                      = 9.999999986944E9
parameter[particle]                 = Positron
parameter[absolute_time_tracking]    = F

beginning[beta_a]    = 50
beginning[beta_b]    = 50


!-------------------------------------------------------

Q1: Quadrupole, L = 1E-10, TILT = 3.2, K1 = 1.2
Q2: Quadrupole, L = 1E-10, TILT = 3.2, K1 = 2.2
Q3: Quadrupole, L = 1E-10, TILT = 3.2, K1 = 0.256

!-------------------------------------------------------
! Overlays, groups, rampers, and superimpose

OV: overlay = {Q1[K1]:{-3, -1, 1, 3, 5, 7}, Q2[K1]:{-2, 0, 2, 4, 6, 8}, Q3[K1]:{-2, 0, 0, 4, 6, 8},
   Q1[TILT]:{-1, 1, 3, 5, 7, 9}, Q2[TILT], Q3[TILT]}, var = {VALUE}, x_knot = {-2, -1, 0, 1, 2, 3},
   VALUE = 0.1
Z: overlay = {OV[SLAVE(2)%Y_KNOT(3)]: a, OV[X_KNOT(5)]: b}, var = {A, B}, A = 2, B = 2

!-------------------------------------------------------
! Lattice lines


LAT1: line = ( Q1, Q2, Q3)

use, LAT1
